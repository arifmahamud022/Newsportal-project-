<!doctype html>
<html lang="en">

    <head>
        <%- include('adminHead'); -%>
    </head>

    <body data-sidebar="dark">

        <!-- <body data-layout="horizontal" data-topbar="dark"> -->

        <!-- Begin page -->
        <div id="layout-wrapper">

            <%- include('adminHeader'); -%>

            <!-- ========== Left Sidebar Start ========== -->
            <div class="vertical-menu">

                <div data-simplebar class="h-100">

                    <!--- Sidemenu -->
                    <%- include('adminLeftSidebar'); -%>
                    <!-- Sidebar -->
                </div>
            </div>
            <!-- Left Sidebar End -->



            <!-- ============================================================== -->
            <!-- Start right Content here -->
            <!-- ============================================================== -->
            <div class="main-content">

                <div class="page-content">
                    <div class="container-fluid">

                        <!-- start page title -->
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                    <h4 class="mb-sm-0">New Post</h4>

                                    <div class="page-title-right">
                                        <ol class="breadcrumb m-0">
                                            <li class="breadcrumb-item"><a href="javascript: void(0);">Upzet</a></li>
                                            <li class="breadcrumb-item active">New Post</li>
                                        </ol>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <!-- end page title -->

                        <!-- start row -->
                        <div class="row">
                            <div class="col-12  d-flex justify-content-center">
                                <% if(flashsms == ''){ }else{ %>
                                <div class="alert alert-success alert-dismissible fade show" role="alert">
                                    <button type="button" class="btn-close" data-bs-dismiss="alert"
                                        aria-label="Close"></button>
                                    <strong><%= flashsms %></strong>
                                </div>
                                <% }; %>
                                <% if(flasherr == ''){ }else{ %>
                                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                    <button type="button" class="btn-close" data-bs-dismiss="alert"
                                        aria-label="Close"></button>
                                    <strong> <%= flasherr %></strong>
                                </div>
                                <% }; %>
                            </div>
                            <div class="col-12 d-flex justify-content-center">
                                <% if(errors){ errors.forEach(function(error){ %>
                                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                    <button type="button" class="btn-close" data-bs-dismiss="alert"
                                        aria-label="Close"></button>
                                    <strong><%= error.msg %></strong>
                                </div>
                                <% }); }; %>
                            </div>
                            <div class="col-12">
                                <div class="card">
                                    <form class="card-body" action="/dashboard/post-add" method="post"
                                        enctype='multipart/form-data'>
                                        
                                        

                                        <div class="mb-3 row">
                                            <label for="name" class="col-md-2 col-form-label">Featured Image</label>
                                            <div class="col-md-10">
                                                <input type="file" class="filestyle" data-input="false"
                                                    name="featured_image" data-buttonname="btn-secondary">
                                            </div>
                                        </div>

                                        

                                        

                                        <div class="mb-3 row">
                                            <div class="col-auto">
                                                <button type="submit" class="btn btn-primary mt-2">Submit</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <!-- end row -->
                    </div>
                    <!-- container-fluid -->
                </div>
                <!-- End Page-content -->

            </div>
            <!-- end main content-->

            <%- include('adminFooter'); -%>

        </div>
        <!-- END layout-wrapper -->

        <!-- JAVASCRIPT -->
        <!-- JAVASCRIPT -->
        <script src="<%= baseUrl; %>/admin/assets/libs/jquery/jquery.min.js"></script>
        <script src="<%= baseUrl; %>/admin/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="<%= baseUrl; %>/admin/assets/libs/metismenu/metisMenu.min.js"></script>
        <script src="<%= baseUrl; %>/admin/assets/libs/simplebar/simplebar.min.js"></script>
        <script src="<%= baseUrl; %>/admin/assets/libs/node-waves/waves.min.js"></script>

        <!-- apexcharts js -->
        <script src="<%= baseUrl; %>/admin/assets/libs/apexcharts/apexcharts.min.js"></script>


        <!--tinymce js-->
        <script src="<%= baseUrl; %>/admin/assets/libs/tinymce/tinymce.min.js"></script>

        <!-- init js -->
        <script src="<%= baseUrl; %>/admin/assets/js/pages/form-editor.init.js"></script>


        <!-- jquery.vectormap map -->
        <script src="<%= baseUrl; %>/admin/assets/libs/jqvmap/jquery.vmap.min.js"></script>
        <script src="<%= baseUrl; %>/admin/assets/libs/jqvmap/maps/jquery.vmap.usa.js"></script>

        <script src="<%= baseUrl; %>/admin/assets/js/pages/dashboard.init.js"></script>

        <script src="<%= baseUrl; %>/admin/assets/js/app.js"></script>

    </body>

</html>